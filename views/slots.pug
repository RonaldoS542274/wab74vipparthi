extends layout

block content
  h1= title
  - var image_names = ["image1.png","image2.png","image3.jpg","image4.png","image5.png"];
    - for(let i =0; i<3; i++){
      - let rand = Math.floor(Math.random()*5)
        img(src=`/images/${image_names[rand]}` alt=`${image_names}` height='300px' width='300px')
  - }
